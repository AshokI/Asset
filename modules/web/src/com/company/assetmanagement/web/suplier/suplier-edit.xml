<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editCaption"
        class="com.company.assetmanagement.web.suplier.SuplierEdit"
        datasource="suplierDs"
        focusComponent="fieldGroup"
        messagesPack="com.company.assetmanagement.web.suplier">
    <dsContext>
        <datasource id="suplierDs"
                    class="com.company.assetmanagement.entity.Suplier"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="suplierDs">
            <column width="250px">
                <field id="supliername"/>
                <field id="state"/>
                <field id="place"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
